# JWT
JWT_SECRET=your-jwt-secret-key-min-32-characters-long

# File Upload
UPLOAD_DIR=./uploads/ideas
MAX_FILE_SIZE=10485760

# CORS
FRONTEND_URL=http://localhost:5173
