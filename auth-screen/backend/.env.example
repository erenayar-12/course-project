# Database
DATABASE_URL="postgresql://user:password@localhost:5432/innovatepam_db"

# Server
PORT=3001
NODE_ENV=development

# Auth0
AUTH0_DOMAIN=your-auth0-domain.auth0.com
AUTH0_CLIENT_ID=your-auth0-client-id
