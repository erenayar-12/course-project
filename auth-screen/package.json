{
  "name": "innovatepam-auth-screen",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --max-warnings 0",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=\\.test\\.tsx?$",
    "test:integration": "jest --testPathPattern=\\.integration\\.test\\.tsx?$",
    "test:coverage": "jest --coverage",
    "test:coverage:report": "jest --coverage && open coverage/lcov-report/index.html || start coverage/lcov-report/index.html",
    "mutation:test": "stryker run",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:spec": "cypress run --spec cypress/e2e/idea-submission.cy.ts",
    "cypress:headless": "cypress run --headless",
    "ci": "npm run type-check && npm run lint && npm run test:coverage && npm run mutation:test"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.2.4",
    "@hookform/resolvers": "^5.2.2",
    "axios": "^1.13.5",
    "express": "^5.2.1",
    "http-proxy-middleware": "^3.0.5",
    "msw": "^2.12.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.71.2",
    "react-router-dom": "^6.20.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.19.33",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "cypress": "^15.11.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.8"
  }
}
